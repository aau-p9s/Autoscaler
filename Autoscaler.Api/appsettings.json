{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Autoscaler": "Information"
    }
  },
  "AllowedHosts": "*",
  "Autoscaler": {
    "Port": 8080,
    "Host": "http://0.0.0.0",
    "Apis": {
      "Kubernetes": {
        "Url": "http://kubernetes",
        "Mock": true
      },
      "Prometheus": {
        "Url": "http://prometheus",
        "Mock": true
      },
      "Forecaster": {
        "Url": "http://forecaster",
        "Mock": true
      }
    },
    "Runner": {
      "Start": false
    },
    "Database": {
      "Hostname": "localhost",
      "Port": 5432,
      "Database": "autoscaler",
      "User": "root",
      "Password": "password"
    }
  }
}
